<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.emndeniz.cputool.Controller">
   <top>
      <VBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </VBox>
   </top>
   <right>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <ScrollPane prefHeight="171.0" prefWidth="215.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="textFieldFilter" layoutY="1.0" prefHeight="25.0" prefWidth="149.0" AnchorPane.leftAnchor="1.0" AnchorPane.topAnchor="1.0" />
                        <Button fx:id="buttonAddFilter" layoutX="150.0" layoutY="1.0" mnemonicParsing="false" onAction="#addFilters" text="Add" AnchorPane.rightAnchor="4.0" />
                     </children></AnchorPane>
               </content>
            </ScrollPane>
         </children>
      </AnchorPane>
   </right>
   <left>
      <HBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="100.0" prefWidth="183.0" spacing="10.0">
               <children>
                  <Button fx:id="buttonStartCpuData" mnemonicParsing="false" onAction="#startCpuDataCollection" text="Start CPU data collection">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="buttonStopCpuData" mnemonicParsing="false" onAction="#stopCpuDataCollection" text="Stop CPU data collection">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Button>
                  <Label fx:id="labelUserCpuUsage" text="User CPU usage">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="labelTotalCpuUsage" layoutX="20.0" layoutY="84.0" text="Total CPU usage">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Label>

               </children>
            </VBox>
            <Button fx:id="buttonExportToExcell" mnemonicParsing="false" text="Export to Excel" />
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </HBox>
   </left>
   <bottom>
      <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="260.0">
         <children>
            <SplitPane dividerPositions="0.5" orientation="VERTICAL" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <items>
                <AnchorPane>
                     <children>
                        <TextArea fx:id="textAreaCpuData" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <padding>
                              <Insets top="15.0" />
                           </padding>
                        </TextArea>
                        <Label text="Adb output:" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                  <ScrollPane>
                     <content>
                        <TextFlow prefHeight="200.0" prefWidth="200.0" />
                     </content>
                  </ScrollPane>
              </items>
            </SplitPane>
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
